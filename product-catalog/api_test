curl -X GET http://localhost:8085/product

curl -X POST -H "Accept: application/json" -H "Content-Type: application/json" --data \
'{"id" : "123456", "title" : "my_title"}' \
http://localhost:8085/product

curl -X POST -H "Accept: application/json" -H "Content-Type: application/json" --data \
'{"id" : "123456", "title" : "my_title_updated"}' \
http://localhost:8085/product

curl -X GET http://localhost:8085/product/123456

curl -X DELETE http://localhost:8085/product/123456
